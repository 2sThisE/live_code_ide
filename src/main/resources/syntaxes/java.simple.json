{
  "scopeName": "source.java",
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#keywords" },
    { "include": "#storage-modifiers" },
    { "include": "#types" },
    { "include": "#primitive-types" },
    { "include": "#support-classes" },
    { "include": "#constants-and-numbers" },
    { "include": "#annotations" }
  ],
  "repository": {
    "comments": {
        "name": "comment.java",
        "match": "//.*|/\\*.*?\\*/"
    },
    "strings": {
        "name": "string.quoted.double.java",
        "match": "\"([^\"\\\\]|\\\\.)*\""
    },
    "keywords": {
      "name": "keyword.control.java",
      "match": "\\b(if|else|for|while|do|switch|case|default|break|continue|return|try|catch|finally|throw|synchronized|new|instanceof|package|import|throws|assert|enum|this|super)\\b"
    },
    "storage-modifiers": {
        "name": "storage.modifier.java",
        "match": "\\b(public|protected|private|static|final|abstract|native|strictfp|transient|volatile)\\b"
    },
    "types": {
      "name": "storage.type.java",
      "match": "\\b(class|interface|extends|implements|void)\\b"
    },
    "primitive-types": {
        "name": "storage.type.primitive.java",
        "match": "\\b(boolean|byte|char|double|float|int|long|short)\\b"
    },
    "support-classes": {
        "name": "support.class.java",
        "match": "\\b(String|System|Object|Integer|Double|Boolean|Character|Byte|Short|Long|Float|List|Map|Set|ArrayList|HashMap|HashSet)\\b"
    },
    "constants-and-numbers": {
      "name": "constant.language.java",
      "match": "\\b(true|false|null|[0-9]+(\\.[0-9]+)?)\\b"
    },
    "annotations": {
        "name": "storage.type.annotation.java",
        "match": "@[A-Z][a-zA-Z0-9_]*"
    }
  }
}