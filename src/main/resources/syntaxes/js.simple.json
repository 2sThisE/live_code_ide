{
  "scopeName": "source.js",
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#keywords" },
    { "include": "#storage-types" },
    { "include": "#constants-and-numbers" },
    { "include": "#support" }
  ],
  "repository": {
    "comments": {
      "name": "comment.js",
      "match": "//.*|/\\*.*?\\*/"
    },
    "strings": {
      "name": "string.quoted.js",
      "match": "(\"([^\"\\\\]|\\\\.)*\"|'([^'\\\\]|\\\\.)*'|`([^`\\\\]|\\\\.)*`)"
    },
    "keywords": {
      "name": "keyword.control.js",
      "match": "\\b(if|else|for|while|do|switch|case|default|break|continue|return|try|catch|finally|throw|new|instanceof|import|export|from|async|await|yield|in|of|this|typeof|delete|void|with)\\b"
    },
    "storage-types": {
      "name": "storage.type.js",
      "match": "\\b(function|var|let|const|class|extends|super)\\b"
    },
    "constants-and-numbers": {
      "name": "constant.language.js",
      "match": "\\b(true|false|null|undefined|NaN|Infinity|[0-9]+(\\.[0-9]+)?)\\b"
    },
    "support": {
      "name": "support.class.js",
      "match": "\\b(console|document|window|Math|JSON|Promise|Object|Array|String|Number|Boolean|Function|Date|RegExp|Error)\\b"
    }
  }
}